[
  {
    "bindings": {
      "cmd-,": null,
      "cmd-h": null,
      "cmd-m": null,
      "cmd-q": null
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-down": "editor::MoveLineDown",
      "cmd-e": "editor::SortLinesCaseInsensitive",
      "cmd-l": "go_to_line::Toggle",
      "cmd-i": "command_palette::Toggle",
      "cmd-shift-d": "editor::DuplicateLineDown",
      "cmd-shift-down": "editor::MoveLineDown",
      "cmd-shift-k": "editor::DeleteLine",
      "cmd-shift-up": "editor::MoveLineUp",
      "cmd-u": "outline::Toggle",
      "cmd-up": "editor::MoveLineUp"
    }
  },
  {
    "context": "Editor && edit_prediction",
    "bindings": {
      "tab": "editor::AcceptEditPrediction"
    }
  },
  {
    "context": "Editor && !edit_prediction",
    "use_key_equivalents": true,
    "bindings": {
      "alt-enter": "editor::ShowEditPrediction"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "cmd-\\": "pane::GoBack",
      "cmd-,": "pane::GoBack"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-i": "command_palette::Toggle"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "alt-cmd-g": "git_panel::ToggleFocus"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "alt-cmd-e": "project_panel::ToggleFocus"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-shift-e": null
    }
  },
  {
    "context": "AgentPanel",
    "bindings": {
      "alt-cmd-e": "project_panel::ToggleFocus"
    }
  },
  {
    "context": "(Editor && mode == full)",
    "bindings": {
      "alt-cmd-e": null
    }
  }
]
